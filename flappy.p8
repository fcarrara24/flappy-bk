__lua__
 	BLOCK_UNIT = 8
	screen_w = 128
	screen_h = 128
	
	f =1 -- frame animation
	flight_slowness = 1 

	FRAME_JUMP = 4
	g = 1.5 -- gravity (each frame)
	animation_up_timer=5;
	t=0

	fall_set={1,2,3,4,5,6,7}
	PIPESET={}
	APPROACH_SPEED = 2

	rndpiperange = 3
	pipespace = 4
	pipeminheight = 6

	player_w = 8
	player_h = 8

	cloud_animation_slowness=1
	cloud_a_counter = 0

	dead_timer =0
	points =0

	-- cd fl/flappy-bk
function _init()
	palt(0, true)   -- imposta il colore 0 come trasparente
	palt(1, false)  -- assicura che il colore 1 (e altri) siano opachi
	resetGame()
end


function _update() 
	create_rnd_cloud()
	if state != "dead" then
			
		if state == "idle" then
			if btnp(4) then
				sfx(0,2)
				state= "up"
				rise()
			else 
				fall()
			end -- button animation pressed
		elseif state == "up" then
			rise()
		end
		
		if count(PIPESET) == 0 then
			create_rnd_pipe()
		else 
			approach_pipe()
		end
	
		detect_collision()
	else
		if not played_death_sfx then
			sfx(1)
			played_death_sfx = true
		end

		dead_timer += 1

		if dead_timer > 0 and btnp(5) then
			resetGame()
		end
	end

end 


function _draw()
	if state != "dead" then
		cls()
		map(1,1,0,0,128,128)
		draw_nuvole(is_move()) -- si muove ogni x frame)
		drawPipe()
		draw_points()
		spr(current_animation,x,y)
	else
		drawDead()
	end
end
-->8

function resetGame() 
	x=10
	y=64
	state="idle" -- idle, up, dead
	PIPESET = {} -- e.x, e.y, e.t
	current_animation = 1
	played_death_sfx = 0
	dead_timer= 0
	points = 0
	init_cloudset()
end

function fall()
	if y < 120 then
		y+=g
	end
end


function rise() 
	if y > 0 then y -= FRAME_JUMP end
	if f == flight_slowness then 
		if current_animation == count(fall_set) then -- reset animation 
			current_animation = 1
			state = "idle"
		else 
			current_animation +=1
		end -- animation update 
		f=0
	end -- flight slowness
	f +=1
end





