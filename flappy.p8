version 41
__lua__
	BLOCK_UNIT = 8
	screen_w = 128
	screen_h = 128
	
	f =1 -- frame animation
	flight_slowness = 1 

	FRAME_JUMP = 4
	g = 1.5 -- gravity (each frame)
	animation_up_timer=5;
	t=0

	fall_set={1,2,3,4,5,6,7}
	PIPESET={}
	APPROACH_SPEED = 2

	rndpiperange = 3
	pipespace = 4
	pipeminheight = 6

	player_w = 8
	player_h = 8

function _init()
	resetGame()
end


function _update() 
	if state != "dead" then
			
		if state == "idle" then
			if btnp(4) then
				state= "up"
				rise()
			else 
				fall()
			end -- button animation pressed
		elseif state == "up" then
			rise()
		end
		
		if count(PIPESET) == 0 then
			create_rnd_pipe()
		else 
			approach_pipe()
		end
	
		detect_collision()
	else 
		if(btnp(5)) then resetGame() end
	end
end 


function _draw()
	if state != "dead" then
		cls()
		map(1,1,1,1,128,128)
		drawPipe()
		spr(current_animation,x,y)
	else
		drawDead()
	end
end
-->8

function resetGame() 
	x=10
	y=64
	state="idle" -- idle, up
	PIPESET = {}
	current_animation = 1
end

function fall()
	if y < 120 then
		y+=g
	end
end


function rise() 
	if y > 0 then y -= FRAME_JUMP end
	if f == flight_slowness then 
		if current_animation == count(fall_set) then -- reset animation 
			print(f)
			current_animation = 1
			state = "idle"
		else 
			current_animation +=1
		end -- animation update 
		f=0
	end -- flight slowness
	f +=1
end





-->8
-- insert and move pipe

function create_rnd_pipe()
	local pipestart = rnd(rndpiperange) +pipeminheight
	local screen_right = screen_w / BLOCK_UNIT -1
	-- first pipe
	for p=0, pipestart -1 do
		if p < pipestart -2 then
			insert_element(screen_right, p, 34) 
			-- mset(screen_right, p, 34)
		else
			insert_element(screen_right, p, 33) 
			-- mset(screen_right,p,33)
		end
	end
	-- last pipe
	for p=pipestart +pipespace, (screen_h / BLOCK_UNIT) +pipespace do
		if p >pipestart +pipespace then
			insert_element(screen_right, p, 34) 
			-- mset(screen_right, p, 34) -- mset(screen_right, p, 34)
		else
			-- mset(screen_right, p, 32)
			insert_element(screen_right, p, 32) 
		end
	end
end

function insert_element(x, y, type) 
	add(PIPESET, {
		x=x,
		y=y,
		t=type
	})
end

function approach_pipe()
	local to_remove = {}
	foreach(PIPESET, function (e)
		e.x -= APPROACH_SPEED/BLOCK_UNIT
		if e.x < 0 then
			add(to_remove, e)
		end
	end)

	-- rimuove dopo il ciclo per evitare conflitti
	foreach(to_remove, function(e)
		del(PIPESET, e)
	end)
end

function drawPipe()
	foreach(PIPESET, function (e)
		spr(e.t, e.x*BLOCK_UNIT, e.y* BLOCK_UNIT)
	end)
end

