-- gestione nuvole e rendering di queste 
cloud_set= {} -- i, w, h, s
active_cloud={} -- {...cloud_set, x, y}
last_fr_generate=0

function init_cloudset()
	cloud_set ={}
	active_cloud = {}

	add(cloud_set, {
		i=28, 
		w=2,
		h=2, 
		s=3
	})
	add(cloud_set, {
		i=24, 
		w=3,
		h=2, 
		s=1
	})
end

function create_rnd_cloud()
	
	if last_fr_generate < 0.9 then
		last_fr_generate +=0.02
	end
	local l = #active_cloud; 
	local prob_range = {0.5, 0.2, 0.2, 0.08,0.08, 0.05, 0.04, 0}
	local prob = prob_range[l+1] * last_fr_generate
	
	if(prob == 0 )return 

	if(rnd(1/prob)) then
		last_fr_generate = 0
		create_cloud()
	end
end

function create_cloud()
	local index = flr(rnd(#cloud_set)) + 1
	local cloud_type = cloud_set[index]
	local direction = flr(rnd(2)) -- 0 = sinistra, 1 = destra
	local d = direction == 0 and -1 or 1
		

	local x_spawn_positions = { (0-cloud_type.w), screen_w }
	
	local cloud_spawn_x = x_spawn_positions[direction +1]

	local cloud_spawn_y = rnd(screen_h)
	
	return
	add(active_cloud, {
		i=cloud_type.i, 
		w=cloud_type.w, 
		h=cloud_type.h, 
		s=cloud_type.s, 
		x=cloud_spawn_x,
		y=cloud_spawn_y, 
		d=d
	})
end

-- in chiamata alla draw
function draw_nuvole(lb_move)
	
	foreach(active_cloud, function(c)
		
		if not c.x then 
			c.x = 0
		end

		if lb_move then
			c.x = c.x - ((c.d * c.s)/8)
		end 

		if(cloud_in_bound(c.x, c.w)) then
			spr(c.i,c.x,c.y,min(c.w, (screen_w-(2*c.w)-c.x)),c.h)
		else 
			del(active_cloud, c)
		end
	end)
end

function is_move() 
	if(cloud_a_counter == cloud_animation_slowness) then
		cloud_a_counter = 0
		return true
	end
		cloud_a_counter +=1
		return false
end 

function cloud_in_bound(x, w) 
	if (
		x < (0-w) or
		x > screen_w 
	) then
		return false
	end
		return true
end

