--gestione punti

function draw_points()
	local w = 40
	local h = 20
	local x = (screen_w -w)/2 
	local y = 10
	rectfill(x, y, x+w, y+h, 0)         -- sfondo nero per il box
	rect(x, y, x+w, y+h, 7)             -- bordo bianco

	print("p: "..pad2(points), x+w/2 -8, y+h/2 -2)
end

function pad2(n)
	local s = tostr(n)
	if (#s < 2) then
		return "0"..s
	else
		return s
	end
end
